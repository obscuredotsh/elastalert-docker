#!/opt/elastalert/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'elastalert==0.2.1','console_scripts','elastalert-create-index'
__requires__ = 'elastalert==0.2.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('elastalert==0.2.1', 'console_scripts', 'elastalert-create-index')()
    )
