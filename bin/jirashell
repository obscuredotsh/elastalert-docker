#!/opt/elastalert/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'jira==3.0a2','console_scripts','jirashell'
__requires__ = 'jira==3.0a2'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('jira==3.0a2', 'console_scripts', 'jirashell')()
    )
